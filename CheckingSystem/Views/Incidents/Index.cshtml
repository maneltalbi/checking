@model IEnumerable<CheckingSystem.Models.Incidents>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class=" span10">

    <div class="module">
        <div class="module-head">
            <h3>List Of Incidents</h3>
        </div>
        <div class="module-body table">


            <table cellpadding="0" cellspacing="0" border="0" class="datatable-1 table table-bordered table-striped  display dataTable" width="100%" id="DataTables_Table_0" aria-describedby="DataTables_Table_0_info" style="width: 100%;">
                <thead>
                    <tr role="row">
                        <th class="sorting_asc" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending" style="width: 135px;">Number</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending" style="width: 197px;">Short Description</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Platform(s): activate to sort column ascending" style="width: 183px;">Caller</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Engine version: activate to sort column ascending" style="width: 115px;">Category</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="CSS grade: activate to sort column ascending" style="width: 83px;">Subcategory</th>
                        <th class="sorting_asc" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending" style="width: 135px;">Business Service</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending" style="width: 197px;">State</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Platform(s): activate to sort column ascending" style="width: 183px;">Priority</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Engine version: activate to sort column ascending" style="width: 115px;">Assignement To</th>

                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="CSS grade: activate to sort column ascending" style="width: 83px;">Update Date</th>
                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="CSS grade: activate to sort column ascending" style="width: 83px;">Update By</th>

                    </tr>
                </thead>
                

                <tbody role="alert" aria-live="polite" aria-relevant="all">
                    @foreach (var item in Model)
                    {
                        <tr class="gradeA even">
                            <td class="  sorting_1">@item.IdInc</td>
                            <td class=" ">@item.Description</td>
                            <td class=" ">@item.Caller</td>
                            <td class="center ">@item.Category</td>
                            <td class="center ">@item.Subcategory</td>
                            <td class="  sorting_1">@item.BusinessService</td>
                            <td class=" ">@item.state</td>
                            <td class=" ">@item.priority</td>
                            <td class="center ">@item.AssignementTo</td>
                            <td class="center ">@item.Updatedate</td>
                            <td class="center ">@item.UbdatedBy</td>
                        </tr>

                    }







                </tbody>
            </table>
        </div>
    </div>



</div>
