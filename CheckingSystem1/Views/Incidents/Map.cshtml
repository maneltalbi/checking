<script src="~/scripts/jquery-3.2.1.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBzfyTgMdy0mW8mC-vAYeeSZmv80wbrF68&callback=initMap&v=weekly"
        async></script>
<style>
    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    #map {
        height: 600px;
        width: 100%;
    }
</style>
<div class="span10">
    <div id="map"></div>

</div>

<script>
    let map;

    function initMap() {
        map = new google.maps.Map(document.getElementById("map"), {
            center: { lat: 35.829300, lng: 10.640630 },
            zoom: 8,
        });
    }

</script>
